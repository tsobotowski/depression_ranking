        <DOC>
            	<DOCNO>s_273_31_3</DOCNO>
            	<TEXT>Do not ask, tell.</TEXT>
        </DOC>
        <DOC>
            	<DOCNO>s_273_31_4</DOCNO>
            	<TEXT>
