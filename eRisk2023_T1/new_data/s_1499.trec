        <DOC>
            	<DOCNO>s_1499_239_0</DOCNO>
            	<TEXT> This is awesome!</TEXT>
        </DOC>
        <DOC>
            	<DOCNO>s_1499_239_1</DOCNO>
            	<TEXT>I am proud of you!</TEXT>
