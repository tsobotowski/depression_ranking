        <DOC>
            	<DOCNO>s_2058_193_0</DOCNO>
            	<TEXT>Im with you.</TEXT>
        </DOC>
        <DOC>
            	<DOCNO>s_2058_193_1</DOCNO>
            	<TEXT>Recently broken up with, moved to a new city, new job.</TEXT>
