        <DOC>
            	<DOCNO>s_2481_403_1</DOCNO>
            	<TEXT>I want one!</TEXT>
        </DOC>
        <DOC>
            	<DOCNO>s_2481_404_0</DOCNO>
            	<TEXT>Very funny.</TEXT>
        </DOC>
        <DOC>
            	<DOCNO>s_2481_404_1</DOCNO>
